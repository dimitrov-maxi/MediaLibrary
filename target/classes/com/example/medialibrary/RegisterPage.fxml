<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="592.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.medialibrary.RegisterPage">
    <children>
        <TextField fx:id="emailInfo" layoutX="62.0" layoutY="305.0" prefHeight="37.0" prefWidth="210.0" promptText="email" />
        <PasswordField fx:id="passInfo" layoutX="62.0" layoutY="354.0" prefHeight="37.0" prefWidth="210.0" promptText="password" />
        <PasswordField fx:id="passInfoVerifier" layoutX="62.0" layoutY="402.0" prefHeight="37.0" prefWidth="210.0" promptText="confirm password" />
        <Button layoutX="62.0" layoutY="456.0" mnemonicParsing="false" onAction="#registerCheck" prefHeight="78.0" prefWidth="210.0" text="Register">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button layoutX="3.0" layoutY="2.0" mnemonicParsing="false" onAction="#goBack" prefHeight="23.0" prefWidth="52.0" text="&lt;---" />
      <TextField fx:id="address" layoutX="62.0" layoutY="257.0" prefHeight="37.0" prefWidth="210.0" promptText="address" />
      <TextField fx:id="mobileNumber" layoutX="60.0" layoutY="208.0" prefHeight="37.0" prefWidth="210.0" promptText="mobile number" />
      <TextField fx:id="EGN" layoutX="60.0" layoutY="158.0" prefHeight="37.0" prefWidth="210.0" promptText="EGN" />
      <TextField fx:id="lastName" layoutX="60.0" layoutY="109.0" prefHeight="37.0" prefWidth="210.0" promptText="last name" />
      <TextField fx:id="firstName" layoutX="60.0" layoutY="60.0" prefHeight="37.0" prefWidth="210.0" promptText="first name" />
    </children>
</AnchorPane>
